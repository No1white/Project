.shopContainer {
  width: 100%;
  height: 100%;
}
.shopContainer .shopInfo {
  width: 100%;
  height: 300rpx;
  color: white;
  margin-top: 30rpx;
}
.shopContainer .shopInfo .shopTitle {
  margin-left: 30rpx;
  font-size: 60rpx;
}
.shopContainer .shopInfo .fans {
  margin-left: 30rpx;
  font-size: 40rpx;
}
.shopContainer .shopInfo .operationBar {
  margin-left: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.shopContainer .shopInfo .operationBar ._span {
  font-size: 80rpx;
}
.shopContainer .shopInfo .operationBar text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 60rpx;
}
.shopContainer .shopInfo .mark {
  width: 100%;
  height: 300rpx;
  position: absolute;
  left: -10%;
  top: 0rpx;
  left: 0rpx;
  z-index: -1;
  background: url(data:image/jpeg;base64,);
  background-position: center center;
  -webkit-filter: blur(30rpx);
          filter: blur(30rpx);
}
.shopContainer .goodsWrap .guessLike {
  font-size: 40rpx;
  margin-left: 20rpx;
  margin-top: 30rpx;
}

