.searhContainer {
  width: 100%;
  height: 100%;
}
.searhContainer .inputWrap {
  margin-left: 20rpx;
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.searhContainer .inputWrap input {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
          flex: 5;
  background: #f2f2f2;
  margin-left: 50rpx;
  height: 70rpx;
}
.searhContainer .inputWrap ._span {
  background: #f2f2f2;
  font-size: 60rpx;
  position: absolute;
}
.searhContainer .inputWrap button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background: #ff0136;
  color: white;
  line-height: 70rpx;
  height: 70rpx;
}
.searhContainer .searchHistory {
  padding-left: 20rpx;
  padding-top: 20rpx;
}
.searhContainer .searchHistory text {
  margin-bottom: 30rpx;
}
.searhContainer .searchHistory .historyContent {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.searhContainer .searchHistory .historyContent view {
  padding: 16rpx;
  margin: 20rpx;
  background: #f5f5f5;
}

